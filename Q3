//Using logical operators, check the greatest out of three numbers
#include<bits/stdc++.h>
using namespace std;

int main() {
int a,b,c;
cout<<"Enter three numbers"<<endl;
cin>>a>>b>>c;
if(a>b and a>c){
cout<<a<<" is the greatest number"<<endl;
}
else if(b>a and b>c){
cout<<b<<" is the greatest number"<<endl;
}
else{
cout<<c<<" is the greatest number"<<endl;
}
return 0;
}
